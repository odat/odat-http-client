{
    "name": "odat/http-client",
    "description": ":package_description",
    "keywords": [
        "laravel",
        "odat",
        "http-client"
    ],
    "homepage": "https://github.com/odat/odat-http-client.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Moath Odat",
            "email": "moad.odat@gmail.com",
            "homepage": "http://odattech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "illuminate/support": "~5.2",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Odat\\HttpClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Odat\\HttpClient\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
